
public class JaggedArray {

	public static void main(String[] args) {
		int q[][] = { 	{ 1, 2, 3, 4 },
						{ 5, 6 },
						{ 9, 8, 7, 6, 8 }
					};// 2-D Array initialization and assignment.
		
System.out.println("======2-D Array||Enhanced For Loop======");
		
		for(int i[]:q) {
			
			for(int j:i) {
				
				System.out.print(j+ " ");
			}
			System.out.println("");
		}

	}

}
